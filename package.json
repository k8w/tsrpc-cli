{
  "name": "tsrpc-cli",
  "version": "2.1.0",
  "description": "TSRPC CLI Tools",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:dts",
    "build:js": "rm -rf dist && npx rollup -c && cp -rf res LICENSE README.md README_CN.md package.json dist/",
    "build:dts": "rm -rf lib && npx tsc && npx api-extractor run --local --verbose && rm -rf lib",
    "try": "node -r ts-node/register src/index.ts proto -i try/proto -o try/proto/proto.ts -n",
    "try:npx": "npx tsrpc proto -i try/proto -o try/proto/proto.ts -n",
    "test": "npx mocha"
  },
  "bin": {
    "tsrpc": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k8w/tsrpc-cli.git"
  },
  "keywords": [
    "tsrpc",
    "tsbuffer",
    "k8w",
    "typescript",
    "buffer",
    "protobuf"
  ],
  "author": "k8w",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/k8w/tsrpc-cli/issues"
  },
  "homepage": "https://github.com/k8w/tsrpc-cli#readme",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.68",
    "@microsoft/api-extractor": "^7.18.19",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/inquirer": "^7.3.3",
    "@types/minimist": "^1.2.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^15.14.9",
    "@types/typescript": "^2.0.0",
    "mocha": "^9.1.3",
    "openapi-types": "^9.3.0",
    "rollup": "^2.59.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "inquirer": "^8.2.0",
    "k8w-extend-native": "^1.4.6",
    "minimist": "^1.2.5",
    "node-json-color-stringify": "^1.1.0",
    "ora": "^5.4.1",
    "os-locale": "^5.0.0",
    "ts-node": "^10.4.0",
    "tsbuffer": "^2.1.0",
    "tsbuffer-proto-generator": "^1.6.0-dev.0",
    "tsbuffer-schema": "^2.1.0",
    "tsrpc-proto": "^1.3.9",
    "typescript": "^4.4.4",
    "typescript-formatter": "^7.2.2"
  }
}
