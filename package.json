{
  "name": "tsrpc-cli",
  "version": "2.0.4-dev.0",
  "description": "TSRPC CLI Tools",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && npx rollup -c && cp LICENSE README.md package.json dist/",
    "try": "npx ts-node src/index.ts proto -i try/proto -o try/proto/proto.ts -n",
    "try:npx": "npx tsrpc proto -i try/proto -o try/proto/proto.ts -n",
    "test": "npx mocha"
  },
  "bin": {
    "tsrpc": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k8w/tsrpc-cli.git"
  },
  "keywords": [
    "tsrpc",
    "tsbuffer",
    "k8w",
    "typescript",
    "buffer",
    "protobuf"
  ],
  "author": "k8w",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/k8w/tsrpc-cli/issues"
  },
  "homepage": "https://github.com/k8w/tsrpc-cli#readme",
  "devDependencies": {
    "@rollup/plugin-replace": "^2.4.2",
    "@types/fs-extra": "^9.0.12",
    "@types/glob": "^7.1.4",
    "@types/inquirer": "^7.3.3",
    "@types/minimist": "^1.2.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^15.14.9",
    "@types/typescript": "^2.0.0",
    "mocha": "^9.1.1",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "inquirer": "^8.1.2",
    "k8w-extend-native": "^1.4.5",
    "minimist": "^1.2.5",
    "node-json-color-stringify": "^1.1.0",
    "ora": "^5.4.1",
    "os-locale": "^5.0.0",
    "ts-node": "^10.2.1",
    "tsbuffer": "^2.0.4",
    "tsbuffer-proto-generator": "^1.4.2",
    "tsrpc-proto": "^1.3.4",
    "typescript": "^4.4.3"
  }
}
