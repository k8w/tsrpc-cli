{
  "name": "tsrpc-cli",
  "version": "2.0.0",
  "description": "TSRPC CLI Tools",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && npx rollup -c && node scripts/copyPackage.js && cp LICENSE README.md dist/",
    "try": "npx ts-node src/index.ts proto -i try/proto -o try/proto/proto.ts -n",
    "try:npx": "npx tsrpc proto -i try/proto -o try/proto/proto.ts -n",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "tsrpc": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k8w/tsrpc-cli.git"
  },
  "keywords": [
    "tsrpc",
    "tsbuffer",
    "k8w",
    "typescript",
    "buffer",
    "protobuf"
  ],
  "author": "k8w",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/k8w/tsrpc-cli/issues"
  },
  "homepage": "https://github.com/k8w/tsrpc-cli#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/glob": "^7.1.3",
    "@types/minimist": "^1.2.1",
    "@types/node": "^12.20.7",
    "@types/typescript": "^2.0.0",
    "colors": "^1.3.3",
    "glob": "^7.1.6",
    "k8w-crypto": "^0.2.0",
    "k8w-extend-native": "^1.4.5",
    "minimist": "^1.2.5",
    "node-json-color-stringify": "^1.1.0",
    "os-locale": "^4.0.0",
    "rollup": "^2.44.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-node": "^9.1.1",
    "tsbuffer": "^2.0.0",
    "tsbuffer-proto-generator": "^1.4.0",
    "tsrpc-proto": "^1.3.1",
    "typescript": "^4.2.3"
  }
}
